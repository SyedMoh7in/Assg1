console.log(cars.num4);
console.log(cars.num5);
console.log(cars.num6);

Q22
//Syntax Error(The array doesn't have a closing square bracket):
let Cars:string[]=["Merc","BMW","Audi","Honda","Volvo","Ford";
console.log(Cars);
//Correct:
let Cars:string[]=["Merc","BMW","Audi","Honda","Volvo","Ford"];
console.log(Cars);

