Q24
//Equality and Inequality
let num1:string = '1';
let num2:string='1';
console.log("Are Num1 and Num2 Equal? I predict True.")
console.log(num1==num2);
let num3:string = '2';
let num4:string='3';
console.log("Are Num3 and Num4 Unequal? I predict True.")
console.log(num3!=num4);

//LowerCase
let Name:string="mohsin"
console.log("Are Names the same alphabatecally? I predict true");
console.log(Name==Name.toLowerCase());
let Name1:string="Mohsin"
console.log("Are Names the same alphabatecally? I predict false");
console.log(Name1==Name1.toLowerCase());

//Less than and greater than
console.log("Is Num3 Greater Num2? I predict True.")
console.log(num3>num2);
console.log("Is Num1 Greater Num2? I predict False.")
console.log(num1>num3);
console.log("Is Num3 Less than Num2? I predict False.")
console.log(num3<num2);
console.log("Is Num1 Less than Num3? I predict True.")
console.log(num1<num3);

//And Or operators
console.log("Num1 and Num2 are 1? I predict true")
console.log(num1&&num2=="1");
console.log("Num1 and Num2 are 2? I predict False")
console.log(num1&&num2=="2");
console.log("Num1 or Num3 are 1? I predict true")
console.log(num1=="1"||num3=="1");
console.log("Num3 or Num4 are 1? I predict false")
console.log(num3=="1"||num4=="1");

//Array
let arr:string[]=["1,3"];
console.log("Does 1 belong to array? I predict false")
console.log("3"==arr[0]);

